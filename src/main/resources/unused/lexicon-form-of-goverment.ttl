@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :formOfGovernment ;
  lemon:entry    :of .

:formOfGovernment a          lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :formOfGovernment ;
  lemon:synBehavior    :formOfGovernment_nounpp ;
  lemon:sense          :formOfGovernment_sense_ontomap .

:formOfGovernment a    lemon:Form ;
  lemon:writtenRep "form of government"@en .

:formOfGovernment_nounpp a           lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:formOfGovernment_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping      :capital_sense_ontomap ;
  lemon:reference        <http://dbpedia.org/ontology/governmentType> ;
  lemon:subjOfProp       :arg2 ;
  lemon:objOfProp        :arg1 ;
  lemon:condition        :formOfGovernment_condition .

:form_of_government a   lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Country> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/governmentType> .

:arg2 lemon:marker :of .

## Prepositions ##

:of a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .
